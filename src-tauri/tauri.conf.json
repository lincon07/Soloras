{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "soloras-tauri",
  "version": "0.1.0",
  "identifier": "com.peakl.soloras-tauri",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "soloras-tauri",
        "focus": true,
        "zoomHotkeysEnabled": false,
        "fullscreen": true
       
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc2Q0NEM0QyODQwQTNGNjMKUldSalB3cUUwdFBNZGpTVkhzeUdKM0YzVEU1S1JTOWNVeTBOWVdZSUVoOTU2WjBNWDBBM2NLcXEK",
      "endpoints": [
        "https://github.com/lincon07/Soloras/releases/latest/download/latest.json"
      ]
    }
  }
}
